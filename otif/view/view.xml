<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="id_car_car_act" model="ir.actions.act_window">
            <field name="name">car</field>
            <field name="type">ir.actions.act_window</field>
            <field name="res_model">car.car</field>
            <field name="view_mode">tree,form</field>
            <field name="help" type="html">
              <p class="oe_view_nocontent_create">
                <!-- Add Text Here -->
              </p><p>
                <!-- More details about what a user can do with this object will be OK -->
              </p>
            </field>
        </record>
        <!-- This Menu Item must have a parent and an action -->
        <menuitem id="id_car_car_menuitem" name="CAR"  action="id_car_car_act" parent="stock.menu_stock_config_settings"/>

         <record model="ir.ui.view" id="id_view_form_picking_stock_ept">
            <field name="name">view.form.picking.stock.ept</field>
            <field name="model">stock.picking</field>
            <field name="inherit_id" ref="stock.view_picking_form"/>
            <field name="arch" type="xml">
                <data>
                	<xpath expr="//field[@name='origin']" position="after">
                		<field name='car_id'/>
                	</xpath>
                </data>
            </field>
        </record>

    </data>
</odoo>